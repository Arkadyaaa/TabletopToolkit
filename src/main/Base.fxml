<VBox prefHeight="720.0" prefWidth="1280.0" styleClass="wholeView" stylesheets="@resources/styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.BaseController">
  <children>
      <HBox fx:id="topBar" prefHeight="60.0" prefWidth="1280.0" />
<AnchorPane fx:id="mainView" minHeight="-1.0" minWidth="-1.0" prefHeight="600.0" prefWidth="1280.0" styleClass="mainViewBG">
  <children>
    <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" style="&#10;" text="View" textAlignment="CENTER" wrapText="false">
               <font>
          <Font size="18.0" fx:id="x1" />
               </font>
               <textFill>
          <Color red="0.624" green="0.624" blue="0.624" fx:id="x2" />
               </textFill>
            </Label>
  </children>
</AnchorPane>
    <HBox id="HBox" fx:id="bottomBar" alignment="CENTER_LEFT" prefHeight="60.0" prefWidth="1280.0" spacing="5.0" styleClass="bottomBar" VBox.vgrow="NEVER">
      <children>
        <Label fx:id="leftStatus" maxHeight="1.7976931348623157E308" maxWidth="-1.0" prefHeight="39.0" prefWidth="590.0" text="Left status" HBox.hgrow="ALWAYS">
          <font>
            <Font size="11.0" fx:id="x3" />
          </font>
          <textFill>
            <Color red="0.625" green="0.625" blue="0.625" fx:id="x4" />
          </textFill>
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
        </Label>
        <Pane prefHeight="50.0" prefWidth="776.0" HBox.hgrow="ALWAYS" />
        <Label fx:id="rightStatus" font="$x3" maxWidth="-1.0" prefHeight="16.0" prefWidth="168.0" text="Right status" textFill="$x4" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Label>
      </children>
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
    </HBox>
  </children>
</VBox>
